{
  "name": "zodgecko",
  "version": "0.1.1-beta.1",
  "description": "Typed Zod schemas and helpers for the CoinGecko API â€” runtime validation + TypeScript types.",
  "keywords": [
    "coingecko",
    "zod",
    "typescript",
    "schemas",
    "validation",
    "crypto",
    "api"
  ],
  "license": "MIT",
  "author": "Simon Hagger",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonhagger/zodgecko.git"
  },
  "homepage": "https://github.com/simonhagger/zodgecko#readme",
  "bugs": {
    "url": "https://github.com/simonhagger/zodgecko/issues"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.js"
    },
    "./endpoints": {
      "types": "./dist/endpoints/index.d.ts",
      "import": "./dist/endpoints/index.js"
    },
    "./fetch": {
      "types": "./dist/fetch/index.d.ts",
      "import": "./dist/fetch/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "check:deps": "madge --circular src",
    "clean": "rimraf dist .tsbuildinfo",
    "build": "tsc -p tsconfig.build.json",
    "docs:registry": "tsx scripts/generate-registry-inventory.ts",
    "gen:registry": "tsx scripts/gen-registry.ts",
    "examples:basic": "tsx examples/basic-usage.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:scripts": "eslint \"scripts/**/*.{ts,tsx}\"",
    "check:registry": "tsx scripts/check-registry-inventory.ts",
    "check:tsdoc": "tsx scripts/enforce-jsdoc-headers.ts",
    "fix:tsdoc": "tsx scripts/enforce-jsdoc-headers.ts --write",
    "check:exports": "tsx scripts/enforce-jsdoc-exports.ts",
    "check:exports:dryrun": "tsx scripts/enforce-jsdoc-exports.ts --dry-run --out jsdoc-preview.txt",
    "fix:exports": "tsx scripts/enforce-jsdoc-exports.ts --write",
    "fix:prettier": "npx prettier src/**/*.ts --write",
    "check:zod": "node scripts/check-zod-version.mjs",
    "check:examples": "tsc -p examples/tsconfig.json",
    "verify:docs": "npm run docs:registry && npm-run-all -l --continue-on-error check:*",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "prepack": "npm run clean && npm run build && npm run test:coverage",
    "prepublishOnly": "npm run clean && npm run build && npm run test:coverage"
  },
  "peerDependencies": {
    "zod": "4.1"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": false
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "madge": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-morph": "^26.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vitest": "^3.2.4",
    "zod": "4.1"
  }
}
