{
  "name": "zodgecko",
  "version": "0.1.1-beta.0",
  "description": "Typed Zod schemas and helpers for the CoinGecko API â€” runtime validation + TypeScript types.",
  "keywords": [
    "coingecko",
    "zod",
    "typescript",
    "schemas",
    "validation",
    "crypto",
    "api"
  ],
  "license": "MIT",
  "author": "Simon Hagger",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonhagger/zodgecko.git"
  },
  "homepage": "https://github.com/simonhagger/zodgecko#readme",
  "bugs": {
    "url": "https://github.com/simonhagger/zodgecko/issues"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.js"
    },
    "./endpoints": {
      "types": "./dist/endpoints/index.d.ts",
      "import": "./dist/endpoints/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CONTRIBUTING.md",
    "TESTING.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rimraf dist .tsbuildinfo",
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "prepack": "npm run clean && npm run build && npm run test:coverage",
    "prepublishOnly": "npm run clean && npm run build && npm run test:coverage"
  },
  "peerDependencies": {
    "zod": "^3.25.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": false
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.2.4",
    "zod": "^4.0.0"
  }
}
